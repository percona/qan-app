{
  "name": "qan-app",
  "version": "v1.17.2",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "scripts": {
    "ng": "PATH=$(npm bin):$PATH; ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "ng lint",
    "format": "tsfmt -r",
    "test": "ng test",
    "test:coverage-report": "open coverage/index.html",
    "e2e": "ng e2e --proxy-config proxy.conf.json",
    "server": "PATH=$(npm bin):$PATH; webpack-dev-server --config webpack.config.js",
    "build": "ng build qan-app  --base-href '/qan/' --aot --output-path 'dist/qan-app' --prod --progress --output-hashing=all",
    "pack": "cd dist/; tar -czf qan-app-`date +%Y%m%dT%H%m%S`.tar.gz qan-app",
    "clean": "rm -r dist/*"
  },
  "private": true,
  "dependencies": {
