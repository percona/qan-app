    "karma-remap-istanbul": "0.6.0",
    "protractor": "5.3.0",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "ts-node": "^5.0.1",
    "tslint": "5.9.1",
    "typescript": "^2.7.2"
  }
