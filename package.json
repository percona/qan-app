    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "@ng-select/ng-select": "^2.20.5",
    "@types/d3-shape": "^1.3.1",
    "@types/jszip": "^3.1.6",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.9",
