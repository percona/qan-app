{
  "name": "qan-app",
  "version": "v1.12.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "scripts": {
    "ng": "PATH=$(npm bin):$PATH; ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e --proxy-config proxy.conf.json",
    "server": "PATH=$(npm bin):$PATH; webpack-dev-server --config webpack.config.js",
    "build": "ng b -bh '/qan/' -pr -aot -op dist/qan-app --environment=prod --output-hashing=all",
    "pack": "cd dist/; tar -czf qan-app-`date +%Y%m%dT%H%m%S`.tar.gz qan-app",
    "clean": "rm -r dist/*"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5.2.6",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.6",
    "@angular/forms": "^5.2.6",
    "@angular/http": "^5.2.6",
    "@angular/platform-browser": "^5.2.6",
    "@angular/platform-browser-dynamic": "^5.2.6",
    "@angular/router": "^5.2.6",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.5",
    "@types/jszip": "^3.1.3",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.5.3",
    "d3": "^4.13.0",
    "font-awesome": "4.7.0",
    "highlight.js": "9.12.0",
    "jszip": "^3.1.5",
    "moment": "^2.20.1",
    "ngx-clipboard": "^10.0.0",
    "numeral": "2.0.6",
    "renderjson": "github:caldwell/renderjson",
    "rxjs": "^5.5.6",
    "ts-helpers": "1.1.2",
    "vkbeautify": "^0.99.3",
    "zone.js": "^0.8.21"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "5.2.10",
    "@types/d3": "5.0.0",
    "@types/highlight.js": "9.12.2",
    "@types/jasmine": "2.8.6",
    "@types/node": "^9.6.0",
    "@types/numeral": "0.0.22",
    "@types/vkbeautify": "0.99.2",
    "codelyzer": "4.1.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "json-server": "^0.12.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
