{
  "$schema": "../node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "https://raw.githubusercontent.com/percona/pmm/PMM-2.0/api/qanpb/json/qanpb.json",
  "output": "src/app/pmm-api-services",
  "prefix": "Api",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "desc",
  "defaultTag": "Api",
  "removeStaleFiles": false,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false
}
